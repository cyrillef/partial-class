{
	"name": "partial-class",
	"version": "0.0.4",
	"description": "Partial Class Experimental Library",
	"scripts": {
		"build:lib": "tsc -p src/lib/tsconfig.json",
		"build:sample": "tsc -p src/sample/tsconfig.json",
		"build": "npm run build:lib && npm run build:sample",
		"run:sample": "node bin-sample/start.js"
	},
	"main": "./bin/index.js",
	"types": "./bin/index.d.ts",
	"exports": {
		".": "./bin/index.js",
		"./package": "./package.json",
		"./package.json": "./package.json"
	},
	"keywords": [
		"Node"
	],
	"author": "Cyrille Fauvel <cyrillefl@free.fr>",
	"license": "MIT",
	"dependencies": {
		"ejs": "^3.1.10",
		"reflect-metadata": "^0.2.2"
	},
	"devDependencies": {
		"@types/node": "^22.9.0",
		"module-alias": "^2.2.3",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.0.4"
	},
	"prettier": {
		"singleQuote": true
	},
	"_moduleAliases": {
		"@/enums": "bin/enums",
		"@/libs": "bin/libs",
		"@/tools": "bin/tools",
		"@": "bin"
	}
}
